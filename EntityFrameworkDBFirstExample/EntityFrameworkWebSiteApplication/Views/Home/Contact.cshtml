@model EntityFrameworkWebSiteApplication.ViewModels.ContactViewModel

<main aria-labelledby="title">
    <h2 id="title">@Model.Title.</h2>
    <h3>@Model.Message</h3>

    <address>
        One Microsoft Way<br />
        Redmond, WA 98052-6399<br />
        <abbr title="Phone">P:</abbr>
        425.555.0100
    </address>

    <address>
        @foreach (var contact in Model.Contacts)
        {
            <strong>@contact.Contact:</strong>   <a href="mailto:@contact.ContactName">@contact.ContactName</a><br />
        }
    </address>
</main>